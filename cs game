	cadeia trOUct
	escrevaMaquina("\nEscolha entre Terrorista ou Contra-Terrorista (TR/CT) ", velocidade)
	leia(trOUct)

	trOUct = t.caixa_baixa(trOUct)
	
		enquanto(trOUct != "tr" e trOUct != "ct"){
			escreva("Digite ", "'tr' ou 'ct' ")
			leia(trOUct)
		}
		
		se(trOUct == "tr"){
			escrevaMaquina("Time escolhido: Terrorista\n", velocidade)
			SeuTime = "Terrorista"
		}
		se(trOUct == "ct"){
			escrevaMaquina("Time escolhido: Contra - Terrorista\n", velocidade)
			SeuTime = "Contra-Terrorista"
		}
	
	}

	// FUNÇÃO PARA A ESCOLHA DO MAPA 
	funcao escolhaMapa(){
		caracter escolhaDeMapa
		limpa()
		escrevaMaquina("Qual mapa deseja jogar?:\n1 - Mirage\n2 - Dust II\n", velocidade)
		leia(escolhaDeMapa)

			enquanto(escolhaDeMapa != '1' e escolhaDeMapa != '2'){
				escreva("Mapa escolhido inválido, escolha entre:\n1 - Mirage\n2 - Dust II")
				leia(escolhaDeMapa)
			}
		
			se(escolhaDeMapa == '1'){
				escrevaMaquina("Mapa escolhido: Mirage\n", velocidade)
				SeuMapa = "Mirage"
				escolhaDeTime()
				mirage()
			}
			senao se(escolhaDeMapa == '2'){
				escrevaMaquina("Mapa escolhido: Dust II\n", velocidade)
				SeuMapa = "Dust II"
				escolhaDeTime()
				
			}
	}

	funcao placar(){
		se(SeuTime == "Terrorista"){
			escreva("\n\n---------------------\nPLACAR DO JOGO:\n\nteʀʀoʀɪsta: ", tr, "\ncoɴtʀa-teʀʀoʀɪsta: ", ct, "\n____________________\n")
		}

		se(SeuTime == "Contra-Terrorista"){
			escreva("\n\n---------------------\nPLACAR DO JOGO:\n\ncoɴtʀa-teʀʀoʀɪsta: ", ct, "\nteʀʀoʀɪsta: ", tr, "\n____________________\n")
		}
	}

	funcao loja(){
		escreva("\nEscolha sua arma, você tem: ", dinheiro, " , escolha com sabedoria.\n")

		se(SeuTime == "Terrorista"){
			inteiro galilsar = 1800
			inteiro ak47 = 2400
			inteiro sg553 = 3000
			inteiro scout = 1700
			inteiro awp = 4750
			escreva("-------------------------\n")
			escreva("     BANCA DO SEU ZÉ   \n\n")
			escreva("GALIL SAR - $1800        \n")
			escreva("AK-47 - $2400            \n")
			escreva("SG 553 - $3000           \n")
			escreva("SCOUT - $1700            \n")
			escreva("AWP - $4750              \n")
			escreva("Sair - $0                \n")
			escreva("_________________________\n")

			inteiro precoArma
			escrevaMaquina("\nDigite o preço da arma que deseja comprar: ", velocidade)
			leia(precoArma)


				se(dinheiro < precoArma){
					escrevaMaquina("Dinheiro insuficiente, tente comprar outra arma: ", velocidade)
					leia(precoArma)
				}
				
				se(precoArma == 0){
					escrevaMaquina("\nFechando a loja...\n", velocidade)
				}

				se(precoArma == 1800 e dinheiro >= 1800){
					SuaArma = "GALIL SAR"
					escrevaMaquina("\nArma adquirida: Galil Sar.", velocidade)
					dinheiro = dinheiro - 1800
				}
				
				se(precoArma == 2400 e dinheiro >= 2400){
					SuaArma = "AK-47"
					escrevaMaquina("\nArma adquirida: AK-47.", velocidade)
					dinheiro = dinheiro - 2400
				}

				se(precoArma == 3000 e dinheiro >= 3000){
					SuaArma = "SG 553"
					escrevaMaquina("\nArma adquirida: SG 553.", velocidade)
					dinheiro = dinheiro - 3000
				}

				se(precoArma == 1700 e dinheiro >= 1700){
					SuaArma = "SCOUT"
					escrevaMaquina("\nArma adquirida: SCOUT.", velocidade)
					dinheiro = dinheiro - 1700
				}

				se(precoArma == 4750 e dinheiro >= 4750){
					SuaArma = "AWP"
					escrevaMaquina("\nArma adquirida: AWP.", velocidade)
					dinheiro = dinheiro - 4750
				}

					enquanto(precoArma != 1800 e precoArma != 2400 e precoArma != 3000 e precoArma != 1700 e precoArma != 4750 e precoArma != 0){
				 	escrevaMaquina("Opção inválida, digite o preço das armas na tabela acima. ", velocidade)
				 	leia(precoArma)
			}
		}

		se(SeuTime == "Contra-Terrorista"){
			inteiro famasf1 = 1800
			inteiro m4a1s = 2400
			inteiro aug43 = 3300
			inteiro scout = 1700
			inteiro awp = 4750
			escreva("-------------------------\n")
			escreva("     BANCA DO SEU ZÉ   \n\n")
			escreva("FAMAS-F1 - $1800        \n")
			escreva("M4A1-S - $2400            \n")
			escreva("AUG-43 - $3000           \n")
			escreva("SCOUT - $1700            \n")
			escreva("AWP - $4750              \n")
			escreva("Sair - $0                \n")
			escreva("_________________________\n")

			inteiro precoArma
			escrevaMaquina("\nDigite o preço da arma que deseja comprar: ", velocidade)
			leia(precoArma)


				se(dinheiro < precoArma){
					escrevaMaquina("Dinheiro insuficiente, tente comprar outra arma: ", velocidade)
					leia(precoArma)
				}
				
				se(precoArma == 0){
					escrevaMaquina("\nFechando a loja...\n", velocidade)
				}

				se(precoArma == 1800 e dinheiro >= 1800){
					SuaArma = "GALIL SAR"
					escrevaMaquina("\nArma adquirida: Galil Sar.", velocidade)
					dinheiro = dinheiro - 1800
				}
				
				se(precoArma == 2400 e dinheiro >= 2400){
					SuaArma = "AK-47"
					escrevaMaquina("\nArma adquirida: AK-47.", velocidade)
					dinheiro = dinheiro - 2400
				}

				se(precoArma == 3000 e dinheiro >= 3000){
					SuaArma = "SG 553"
					escrevaMaquina("\nArma adquirida: SG 553.", velocidade)
					dinheiro = dinheiro - 3000
				}

				se(precoArma == 1700 e dinheiro >= 1700){
					SuaArma = "SCOUT"
					escrevaMaquina("\nArma adquirida: SCOUT.", velocidade)
					dinheiro = dinheiro - 1700
				}

				se(precoArma == 4750 e dinheiro >= 4750){
					SuaArma = "AWP"
					escrevaMaquina("\nArma adquirida: AWP.", velocidade)
					dinheiro = dinheiro - 4750
				}

					enquanto(precoArma != 1800 e precoArma != 2400 e precoArma != 3000 e precoArma != 1700 e precoArma != 4750 e precoArma != 0){
				 	escrevaMaquina("Opção inválida, digite o preço das armas na tabela acima. ", velocidade)
				 	leia(precoArma)
			}
		}
	}

	funcao escolhaErrada(){
		enquanto(escolha1 != '1' e escolha1 != '2'){
			escrevaMaquina("\nPor favor, forneça uma opção válida.\n", velocidade)
			leia(escolha1)
		}
	}
	
	// FUNCOES COM OS MAPAS E O JOGO

	funcao mirage(){
		se(SeuTime == "Terrorista"){
			cadeia ondeRushar = "a"

			 limpa()

			 se(rodada == 0){
			 	 escrevaMaquina("Vai rushar A, MID no fds ou B já metendo o loko? ", velocidade)
				 leia(ondeRushar)
				 ondeRushar = t.caixa_baixa(ondeRushar)
			 }

			 se(rodada == 1){
			 	 escrevaMaquina("Vai avançar A, MID com vontade ou B na paz? ", velocidade)
				 leia(ondeRushar)
				 ondeRushar = t.caixa_baixa(ondeRushar)
			 }

			 se(rodada == 2){
			 	 escrevaMaquina("Vai forsar A, MID rebolando lentinho ou B com vontade? ", velocidade)
				 leia(ondeRushar)
				 ondeRushar = t.caixa_baixa(ondeRushar)
			 }

			 se(rodada == 3){
			 	 escrevaMaquina("Vai fazer absolute cinema na A, absolute cinema no MID ou absolute cinema na B? ", velocidade)
				 leia(ondeRushar)
				 ondeRushar = t.caixa_baixa(ondeRushar)
			 }

			 se(rodada == 4){
			 	placar()
			 	escrevaMaquina("\n\nFim de jogo!\n", velocidade)
			 	escrevaMaquina("\nisso porque seu pc crashou o cs quando você iria fazer highlight e pegou fogo.", velocidade)
			 }

				 enquanto(ondeRushar != "a" e ondeRushar != "mid" e ondeRushar != "b"){
			 		escrevaMaquina("Opção inválida, escolha entre 'a', 'mid' ou 'b' ", velocidade)
			 		leia(ondeRushar)
				 }


		 	 se(ondeRushar == "a"){
		 	 	cadeia palacioOUcaverna
		 		escrevaMaquina("Ok, vamos rushar pra A, mas vai subir Palácio ou descer Caverna? ", velocidade)
		 		leia(palacioOUcaverna)
		 		palacioOUcaverna = t.caixa_baixa(palacioOUcaverna)
		 		
		 			enquanto(palacioOUcaverna != "palacio" e palacioOUcaverna != "caverna"){
		 				escrevaMaquina("Opção inválida, escolha entre 'palacio' ou 'caverna' ", velocidade)
		 				leia(palacioOUcaverna)
		 			}
		 			
//PALACIOOOO
		 		
		 			se(palacioOUcaverna == "palacio"){
		 				//ROUND 1 PALACIO
		 					se(rodada == 0){
		 						limpa()
