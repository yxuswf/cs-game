programa
{
	inclua biblioteca Texto --> t
	inclua biblioteca Util --> u
	inteiro velocidade = 25
	cadeia SeuTime, SeuMapa
	cadeia SuaArma = "Pistola"
	caracter escolha1 = '0'
	inteiro ct = 0
	inteiro tr = 0
	inteiro rodada = 0
	inteiro dinheiro = 0

	
	funcao inicio(){
	
	escrevaMaquina("Olá futuro fallen2.0!\n", velocidade)
	escrevaMaquina("Você está no Counter Strike, um jogo foda de tiro que você provavelmente já ouviu falar e fingiu que joga todo dia.\n", velocidade)
	escrevaMaquina("Neste jogo text game você pode escolher entre 2 mapas (apenas 2 por falta de tempo) que são o Mirage, e o Dust II.\n", velocidade)
	escrevaMaquina("Ao informar o mapa que deseja jogar você poderá escolher entre TR (Terrorista) ou CT (Contra-Terrorista).\n", velocidade)
	
	cadeia iniciarOjogo
	escreva("Deseja iniciar o jogo? (s/n) ")
	leia(iniciarOjogo)

	iniciarOjogo = t.caixa_baixa(iniciarOjogo)

		enquanto(iniciarOjogo != "s" e iniciarOjogo != "n"){
			escreva("Opção inválida, escolha entre 's' ou 'n' ")
			leia(iniciarOjogo)
		}
		
		se(iniciarOjogo == "s"){
			limpa()
			escrevaMaquina("Antes de iniciar o jogo, precisamos instalar o jogo na sua máquina, vamos verificar para ver se ela suporta o cs: \n", velocidade)
				escrevaMaquina("\nNome do Dispositivo	DESKTOP-PCDAESCOLA", velocidade)
				escrevaMaquina("\nProcessador	Intel(R) Pentium IV  CPU @ 1.30GHz   2.00 GHz", velocidade)
				escrevaMaquina("\nRAM instalada	4,00 GB (utilizável: 2,87 GB)", velocidade)
				escrevaMaquina("\nArmazenamento	64 GB HDD", velocidade)
				escrevaMaquina("\nPlaca de vídeo	Nvidia GeForce 2", velocidade)
			escrevaMaquina("\n\nPelo menos tem 64 GB de armazenamento.", velocidade)
			escrevaMaquina("\nVerificando se sua máquina roda cs...", velocidade)
			escrevaMaquina("\n...", velocidade)
			escrevaMaquina("\nSerá que da pra baixar cs num Windows 2000?", velocidade)
			escrevaMaquina("\nVamos pesquisar no youtoba", velocidade)
			escrevaMaquina("\nVocê baixou um apk com link direto da mediafire atualizado e conseguiu baixar um cs pra poder se divertir!!", velocidade)
			escrevaMaquina("\n(juntamente de 39 vírus)", velocidade)
			escrevaMaquina("\n\nIniciando o jogo...", velocidade)
			escreva("\n3..")
			escreva("\n2..")
			escreva("\n1..")
			escreva("\nta travandooooo, ta rodando a -16 fps")

escrevaMaquina("                                                                                                     \n", 0)
escrevaMaquina("                                                                                                     \n", 0)
escrevaMaquina("                                                 █████                                               \n", 0)
escrevaMaquina("                                                ███████                                              \n", 0)
escrevaMaquina("                                             █████████████████████                                   \n", 0)
escrevaMaquina("                                            ██████████████████████████                               \n", 0)
escrevaMaquina("                                           ██████████████████                                        \n", 0)
escrevaMaquina("                                           ██████████████████                                        \n", 0)
escrevaMaquina("                                          ██████████████████                                         \n", 0)
escrevaMaquina("                                          █████████████ ██                                           \n", 0)
escrevaMaquina("                                           ████████████                                              \n", 0)
escrevaMaquina("                                          ████████████                                               \n", 0)
escrevaMaquina("  ███████████████    ███████████████      ███████████        ████████████████   ███████████████ ████ \n", 0)
escrevaMaquina(" █████████████████  █████████████████     ████████████      █████████████████   ████████████████     \n", 0)
escrevaMaquina(" ██████             █████                 █████████████     ████               █████        ████     \n", 0)
escrevaMaquina(" ██████             ████████████████      █████  ███████    ████  ████████████ █████        ████     \n", 0)
escrevaMaquina(" ██████              ████████████████     █████   ███████   ████   ███████████ █████        ████     \n", 0)
escrevaMaquina(" ██████                          ████    █████      █████   ████          ████ █████        ████     \n", 0)
escrevaMaquina("  ████████████████  █████████████████   █████       █████   ██████████████████  ████████████████     \n", 0)
escrevaMaquina("   ██████████████    ███████████████   █████        █████     ██████████████     ██████████████      \n", 0)
escrevaMaquina("                                       ████          ████                                            \n", 0)
escrevaMaquina("                                      ████           ████                                            \n", 0)
escrevaMaquina("                                      ████          ███████                                          \n", 0)
escrevaMaquina("                                       ███                                                           \n", 0)

		escrevaMaquina(" \n", 1)
		escolhaMapa()
			
		} 
		
		senao se(iniciarOjogo == "n"){
			escreva("Ok, deletando 'C://Windows//System32' ou talvez 'sudo rm -rf --no-preserve-root /'")
		}
	}
	
	funcao escrevaMaquina(cadeia texto, inteiro velocidade){
		para(inteiro i=0; i < t.numero_caracteres(texto); i++){
			escreva(t.obter_caracter(texto, i))
			u.aguarde(velocidade)
		}
	}
	
     // FUNÇÃO PARA A ESCOLHA DE TIMES
	funcao escolhaDeTime(){
escreva("                       ░░░░░░░░░░                                                                                              ██████████                        \n")
